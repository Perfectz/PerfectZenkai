{
  "name": "perfect-zenkai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "dev:mobile": "concurrently \"npm run dev\" \"ngrok http 5173\"",
    "dev:tunnel": "ngrok http 5173",
    "dev:restart": "powershell -ExecutionPolicy Bypass -File scripts/restart-dev.ps1",
    "dev:restart:bash": "bash scripts/restart-dev.sh",
    "dev:https": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "preview:stable": "npm run build && npm run preview -- --port 4173 --host",
    "preview:https": "npm run build && npm run preview -- --port 4173 --host",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:cleanup": "powershell -ExecutionPolicy Bypass -File scripts/kill-port.ps1 -Port 5173",
    "e2e": "npm-run-all test:cleanup --parallel dev \"e2e:run --\"",
    "e2e:run": "wait-on https://localhost:5173 && playwright test",
    "e2e:single": "npm-run-all test:cleanup --parallel dev \"e2e:run:single\"",
    "e2e:run:single": "wait-on https://localhost:5173 && playwright test e2e/AuthViaKeyVault.e2e.ts",
    "e2e:ui": "playwright test --ui",
    "build:preview": "npm run build && npm run preview",
    "deploy:vercel": "npm run build && npx vercel --prod",
    "deploy:netlify": "npm run build && npx netlify deploy --prod --dir=dist",
    "deploy:gh-pages": "npm run build && npx gh-pages -d dist",
    "lighthouse": "node scripts/lighthouse.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:scan": "eslint . --ext .ts,.tsx",
    "build:secure": "npm run security:audit && npm run build",
    "prepare": "husky install",
    "postinstall": "playwright install"
  },
  "dependencies": {
    "@azure/mcp": "^0.2.2",
    "@google-cloud/local-auth": "^3.0.1",
    "@langchain/core": "^0.3.58",
    "@langchain/openai": "^0.5.13",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "date-fns": "^4.1.0",
    "dexie": "^3.2.4",
    "eventemitter3": "^5.0.1",
    "google-auth-library": "^10.1.0",
    "jwt-decode": "^4.0.0",
    "langchain": "^0.3.28",
    "lucide-react": "^0.294.0",
    "openai": "^5.3.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-swipeable": "^7.0.2",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.64",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.3.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react-router-dom": "^7.6.2",
    "supabase": "^2.24.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.2.3",
    "wait-on": "^7.2.0",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  }
}
